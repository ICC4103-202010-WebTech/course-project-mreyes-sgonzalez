<p id="notice"><%= notice %></p>

<h1>Events</h1>
<td><%= link_to "Add", new_event_path, class: ["btn", "btn-primary"] %></td>
<table>
  <thead>
    <th>Title</th>
    <th>Description</th> 
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.title, event %></td>
        <td><%= event.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

