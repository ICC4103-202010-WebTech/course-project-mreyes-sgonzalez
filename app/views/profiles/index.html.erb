<p id="notice"><%= notice %></p>

<h1>Profiles</h1>
<link rel="stylesheet" type="text/css" href="event_id_view.css">
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Date of Birth</th>
      <th>Address</th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.dob %></td>
        <td><%= profile.address %></td>
        <td><button class="btn btn-primary" id="edit_button_event">Edit</button></td>
        <td><button class="btn btn-primary" id="delete_button_event">Delete</button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

